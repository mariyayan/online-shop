{{>menu productsData=productsData showUserNavMenu=showUserNavMenu}}
<section id='search-container' class='wrapper'>
    <div class='flex cards-block-container'>
        {{#each products}}
        	{{>product-card product=this classes=true sales=false}}
        {{else}}
        	<p>По вашему запросу ничего не найдено</p>
        {{/each}}
    </div>
</section>